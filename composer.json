{
    "name": "cerebralfart/laravel-crud-controller",
    "description": "An easy way to do your CRUD stuff",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cerebralfart\\LaravelCRUD\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cerebralfart\\LaravelCRUD\\Test\\": "test/"
        }
    },
    "authors": [
        {
            "name": "Sven Mol",
            "email": "sven@devcake.nl"
        }
    ],
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0"
    },
    "scripts": {
        "post-autoload-dump": [
            "@php ./vendor/bin/testbench package:discover --ansi"
        ]
    }
}
